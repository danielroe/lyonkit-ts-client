{
  "name": "lyonkit-ts-client",
  "type": "module",
  "version": "2.6.3",
  "private": "true",
  "packageManager": "pnpm@8.13.1",
  "description": "Lyonkit typescript clients and related packages",
  "author": "LÃ©o Coletta <contact@leo-coletta.fr>",
  "scripts": {
    "build": "rimraf packages/*/dist && pnpm -r run build",
    "release": "standard-version && git push --follow-tags origin main && pnpm -r run publish",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "pnpm -r run typecheck",
    "@ts-client": "pnpm --filter @lyonkit/ts-client",
    "@nuxt": "pnpm --filter @lyonkit/nuxt"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.6.1",
    "bumpp": "^9.2.1",
    "changelogen": "^0.5.5",
    "eslint": "^8.56.0",
    "typescript": "^5.3.3"
  },
  "volta": {
    "node": "20.10.0",
    "npm": "10.2.5"
  },
  "dependencies": {
    "standard-version": "^9.5.0"
  },
  "standard-version": {
    "packageFiles": [
      {
        "filename": "packages/*/package.json",
        "type": "json"
      }
    ]
  }
}
